<br>

<table class="table">
  <tr>
    <th class="text-center">Date</th>
    <th class="text-center">Category</th>
    <th class="text-center">Sum</th>
    <th class="text-center">Description</th>
    <th class="text-center">X</th>
    <th class="text-center">E</th>
  </tr>

  <tr>
    <td><%= @earning.date %></td>
    <td><%= @earning.category %></td>
    <td><%= number_with_delimiter(@earning.sum) %></td>
    <td><%= @earning.description %></td>
    <td><%= link_to 'X', delete_earning_path(params[:user_id], params[:id]), method: :delete, data: {confirm: 'Are you sure?'} %></td>
    <td><%= link_to 'E', edit_user_earning_path(@earning.user, @earning.id) %></td>
  </tr>
</table>